{
  "name": "pollen-solutions/http",
  "description": "Pollen Http Component - HTTP specification layer.",
  "homepage": "https://www.presstify.com/pollen-solutions/http/",
  "keywords": [
    "pollen-solutions",
    "component",
    "http",
    "PSR-7"
  ],
  "time": "2021-05-11",
  "license": "MIT",
  "authors": [
    {
      "name": "Jordy Manner",
      "email": "jordy@presstify.com",
      "role": "Developper"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.presstify.com/pollen-solutions/"
    }
  ],
  "require": {
    "php": ">=7.4",
    "composer/composer": "^2.0",
    "composer/installers": "~1.0",
    "league/uri": "^6.4",
    "league/uri-components": "^2.3",
    "nyholm/psr7": "^1.3",
    "pollen-solutions/support": "^1.0",
    "symfony/http-foundation": "^5.2",
    "symfony/mime": "^5.2",
    "symfony/psr-http-message-bridge": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Pollen\\Http\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pollen\\Http\\Test\\": "tests"
    }
  },
  "suggest": {
    "pollen-solutions/container" : "Pollen Container Component - PSR-11 ready Dependencies Injection Container.",
    "pollen-solutions/cookie": "Pollen Cookie Component - Cookies repository.",
    "pollen-solutions/session": "Pollen Session Component - HTTP Request User's session store and query information."
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
